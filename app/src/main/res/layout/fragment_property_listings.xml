<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".view.fragment.PropertyListings">

    <LinearLayout
        android:id="@+id/search_container"
        android:layout_width="350dp"
        android:layout_height="60dp"
        android:orientation="horizontal"
        android:background="@drawable/search_bar_background"
        android:padding="8dp"
        android:layout_gravity="center_horizontal"
    android:layout_marginTop="40dp"
    android:layout_marginStart="16dp"
    android:layout_marginEnd="16dp">

        <EditText
            android:id="@+id/searchBar"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:drawableStart="@drawable/search_icon_"
            android:drawableEnd="@drawable/filter_icon"
            android:hint="Search"
            android:padding="10dp"
            android:background="@null"
            />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/house_container"
        android:layout_width="110dp"
        android:layout_height="120dp"
        android:layout_gravity="left"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="120dp"
    android:background="@drawable/search_bar_background"
    android:gravity="center"
    android:orientation="vertical"
    android:padding="8dp"
        android:clickable="true"
        android:focusable="true">

    <ImageView
        android:id="@+id/house_icon"
        android:layout_width="75dp"
        android:layout_height="75dp"
        android:src="@drawable/baseline_home_filled_24" />

    <TextView
        android:id="@+id/house_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:gravity="center"
        android:text="House"
        android:textAlignment="center"
        android:textSize="16sp" />
</LinearLayout>

    <LinearLayout
        android:id="@+id/rental_container"
        android:layout_width="110dp"
        android:layout_height="120dp"
        android:layout_gravity="left"
        android:layout_marginLeft="150dp"
        android:layout_marginTop="120dp"
        android:background="@drawable/search_bar_background"
        android:gravity="center"
        android:orientation="vertical"
        android:padding="8dp"
        android:clickable="true"
        android:focusable="true">

        <ImageView
            android:id="@+id/rental_icon"
            android:layout_width="75dp"
            android:layout_height="75dp"
            android:src="@drawable/baseline_apartment_24" />

        <TextView
            android:id="@+id/rental_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:gravity="center"
            android:text="Rent"
            android:textAlignment="center"
            android:textSize="16sp" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/land_container"
        android:layout_width="110dp"
        android:layout_height="120dp"
        android:layout_gravity="left"
        android:layout_marginLeft="280dp"
