        <TextView
            android:id="@+id/land_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:gravity="center"
            android:text="Land"
            android:textAlignment="center"
            android:textSize="16sp" />

    </LinearLayout>


